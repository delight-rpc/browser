{
  "name": "@delight-rpc/browser",
  "version": "4.0.0",
  "description": "",
  "keywords": [],
  "files": [
    "lib"
  ],
  "main": "lib/es2018/index.js",
  "types": "lib/es2018/index.d.ts",
  "repository": "git@github.com:delight-rpc/browser.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "karma start karma.config.js --single-run",
    "test:dev": "karma start karma.config.js",
    "prepublishOnly": "run-s clean build bundle",
    "clean": "run-p clean:*",
    "clean:build": "rimraf lib",
    "clean:bundle": "rimraf dist",
    "build": "run-p build:*",
    "build:es2015": "run-s build:es2015:*",
    "build:es2015:compile": "tsc --project tsconfig.build.json --module commonjs --target es2015 --outDir lib/es2015",
    "build:es2015:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2015",
    "build:es2018": "run-s build:es2018:*",
    "build:es2018:compile": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib/es2018",
    "build:es2018:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2018",
    "bundle": "rollup --config rollup.config.js",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint test",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/jasmine": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "cross-env": "^7.0.3",
    "delight-rpc": "^4.0.0",
    "eslint": "8.15.0",
    "extra-filesystem": "^0.4.2",
    "husky": "4",
    "karma": "^6.3.19",
    "karma-chrome-launcher": "^3.1.1",
    "karma-jasmine": "^5.0.0",
    "karma-webpack": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "return-style": "^1.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.72.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.2",
    "ts-loader": "^9.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tscpaths": "^0.0.9",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0"
  },
  "dependencies": {
    "@blackglory/errors": "^2.2.1",
    "@delight-rpc/protocol": "^2.2.0",
    "extra-promise": "^1.0.2"
  },
  "peerDependencies": {
    "delight-rpc": "^4.0.0"
  }
}
